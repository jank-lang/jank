((fn* []
      (let* [outer-count (atom 0)
             f (fn* outer [fn?]
                    (swap! outer-count inc)
                    (when fn?
                      (fn* inner []
                           (outer false))))]
            (assert (= nil (f false)))
    (assert (= 1 @outer-count))
(reset! outer-count 0)
(assert (= nil ((f true))))
    (assert (= 2 @outer-count))
   :success)))
