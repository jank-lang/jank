(def count*
  (fn* [& args]
       (loop [args args
              n 0]
         (if (empty? args)
           n
           (do
          ;(println (first args))
             (recur (rest args) (+ n 1)))))))

(assert (= 0 (count*)))
(assert (= 4 (count* 1 2 3 :four)))
(assert (= 5 (count* 1 2 3 :four 5)))

:success
