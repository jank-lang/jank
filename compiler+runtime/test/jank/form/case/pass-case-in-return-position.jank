; The `case` is the return value of a function
(assert
  (=
    ((fn []
       (case 2
         1 :one
         2 :two
         :default)))
    :two))

(assert
  (=
    ((fn []
       (case "hello"
         "world" :world
         "hello" :greeting
         :default)))
    :greeting))

(assert
  (=
    ((fn []
       (case true
         true  :yes
         false :no
         :default)))
    :yes))

(assert
  (=
    ((fn []
       (case 3.14
         3.14 :pi
         2.71 :e
         1.61 :phi
         :default)))
    :pi))

:success
