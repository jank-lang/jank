; test case in value position
(let [xfn
      (case 1
        1 :one
        1 :duplicate-one
        :default)]
  (assert (= xfn :one)))

; This will fail due to duplicate keys

; test case in statement position
(let [xfn (fn []
            (case 1
              1 :one
              1 :duplicate-one
              :default)
            :arbitrary-value)]
  (assert (= (xfn) :arbitrary-value)))

; This will also fail due to duplicate keys

; test case in return position
(assert
  (=
    (case 1
      1 :one
      1 :duplicate-one
      :default)
    :one))

; This will fail due to duplicate keys
