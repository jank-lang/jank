;; Test catching C++ float exception
(cpp/raw "namespace jank::cpp::try_::pass_catch_cpp_float {
  void throw_float_half() { throw 0.5f; }
}")

(assert
 (= 0.5
    (try
      (cpp/jank.cpp.try_.pass_catch_cpp_float.throw_float_half)
      (catch cpp/double d -1.0)
      (catch cpp/float f f))))

:success
