(def m {:wow 1})

; Metadatable
(assert (= {:status :good} (with-meta {:status :good} m)))
(assert (= m (meta (with-meta {:status :good} m))))

(assert (= [:a :b] (with-meta [:a :b] m)))
(assert (= m (meta (with-meta [:a :b] m))))

(assert (= 'foo (with-meta 'foo m)))
(assert (= m (meta (with-meta 'foo m))))

(assert (= "foo" (with-meta "foo" m)))
(assert (= m (meta (with-meta "foo" m))))

